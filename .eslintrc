{
    "env": {
        "node": true,
        "mocha": true
    },
    "rules": {
        "camelcase": [0],
        "comma-dangle": [2],
        "comma-spacing": [2],
        "consistent-return": [2],
        "curly": [0],
        "dot-notation": [2],
        "eqeqeq": [2],
        "key-spacing": [2],
        "max-len": [1, 360],
        "new-cap": [0],
        "no-bitwise": [2],
        "no-caller": [0],
        "no-console": [2],
        "no-empty": [0],
        "no-multi-spaces": [2],
        "no-proto": [0],
        "no-redeclare": [2],
        "no-shadow": [0],
        "no-redeclare": [2],
        "no-trailing-spaces": [2],
        "no-undef": [0],
        "no-use-before-define": [0],
        "no-unused-vars": [2],
        "no-underscore-dangle": [0],
        "quotes": [2, "single"],
        "semi": [2, "always"],
        "space-after-keywords": [2, "always"],
        "space-before-blocks": 2,
        "space-before-function-paren": [2, "never"],
        "space-in-parens": [2, "never"],
        "strict": [0]
    }
}
